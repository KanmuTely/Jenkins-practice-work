pipeline {
    agent any
    stages {
        stage('my first stage'){
            steps{
                echo "from visual studio"
            }
             
            }
            stage('input'){
            steps{
                input("from visual studio")}
        }
        stage('next'){
        parallel{
            stage('parallel check1'){
            steps{
                echo "from visual studio"}
        }stage('parallel check2'){
            steps{
                echo "from visual studio"}
        }
        }
           
    }
}